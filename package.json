{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"app/**/*.{ts,js,tsx,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,js,tsx,jsx}\" --fix",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install"
  },
  "lint-staged": {
    "./app/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-pattern .gitignore --fix",
      "prettier --ignore-pattern .gitignore --write"
    ]
  },
  "dependencies": {
    "next": "15.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "@tailwindcss/postcss": "^4.1.5",
    "@types/node": "^22.15.12",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3"
  }
}
